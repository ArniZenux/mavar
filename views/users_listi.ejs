<%- include('header') -%>

<br>

<h2 class="header__title"><%= subtitle %></h2>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Nafn</th>
      <th>Sími</th>
      <th>Netfang</th>
      
      <th class="d-print-none">
        <!-- <a class="btn btn-sm btn-success" href="addusers">Bæta nýr túlk</a>-->
      </th>
    </tr>
  </thead>
  <tbody>
    <% for(let user of model) { %>
     <tr>
       <td><%= user.NAFN %></td>
       <td><%= user.SIMI %></td>
       <td><%= user.NETFANG %></td>
       <td class="d-print-none">
         <a class="btn btn-sm btn-warning" href="user_pickup/<%= user.KT %>">Skoða</a>
         
        </td>
     </tr> 
     <% } %> 
  </tbody>
</table>

<br>

<%- include('footer') -%> 